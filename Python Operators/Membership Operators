#in            True if value is found in the sequence
#not in        True if value is not found in the sequence

a=10
b=20
list=[10,30,50,70]

if(a not in list):
    print("a is not in list")
else:
    print("a is in list")

if(b in list):
    print("b is in list")
else:
    print("b is not in list")
